<style>


</style>

<h1>reporte</h1>
<div class="row">
    <div class="col s12">
         <div class="card">
              <div class="card-content ">
 <table class="responsive-table" >
        <thead>
          <tr>
              <th>Factura</th>
              <th>Estado</th>
              <th>Area</th>
              <th>Cheque</th>
               <th>Banco</th>
               <th>Email contacto</th>
               <th>Telefono contacto</th>
               <th>Comentario</th>
               <th>acciones</th>
          </tr>
        </thead>
        <tbody>
            {{#each data }}
            {{#if data.estado = 'Deposito'}}
                <tr class="green darken-1 white-text">
                    <td>{{factura}}</td>
                    <td>{{estado}}</td>
                    <td >{{area}}</td >
                    <td>${{cheque}}</td>
                    <td>{{banco}}</td>
                    <td>{{emailcontacto}}</td>
                    <td>{{fonocontacto}}</td>
                    <td>{{comentario}}</td>
                    <td><a class="waves-effect waves-light btn-small">Editar</a><a class="waves-effect red darken-4 btn-small">Eliminar</a></td>
                </tr>
                {{/if }}
            {{/each}}
        </tbody>
      </table>
    </div>
</div>
</div>
</div>
  